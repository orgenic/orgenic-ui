{
  "name": "@orgenic/orgenic-ui",
  "version": "0.2.1",
  "description": "ORGENIC UI",
  "author": {
    "name": "MAXIMAGO GmbH",
    "email": "dev@maximago.de"
  },
  "homepage": "https://orgenic.org/",
  "license": "MIT",
  "keywords": [
    "web components",
    "components",
    "custom elements",
    "user experience",
    "UX",
    "themes"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orgenic/orgenic-ui.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "unpkg": "dist/orgenic-ui.js",
  "jsdelivr": "dist/orgenic-ui.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build.watch": "stencil build --docs --watch",
    "start": "stencil build --dev --watch --serve --config stencil.run.config.ts",
    "test": "stencil test --spec --e2e",
    "test.ci": "stencil test --spec --e2e --ci",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "check-licenseheader": "node ./.scripts/check-licenseheaders.js"
  },
  "pre-commit": [
    "check-licenseheader"
  ],
  "dependencies": {},
  "devDependencies": {
    "@stencil/core": "^0.18.1",
    "@stencil/sass": "^0.1.1",
    "@types/jest": "23.3.14",
    "@types/puppeteer": "1.11.1",
    "@types/tabulator-tables": "^4.2.3",
    "copyfiles": "^2.1.0",
    "glob": "^7.1.3",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "pre-commit": "^1.2.2",
    "puppeteer": "1.11.0",
    "tabulator-tables": "^4.2.3"
  }
}
